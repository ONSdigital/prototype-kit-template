---
group: Examples
layout: example/views/_master
---
{% from "components/button/_macro.njk" import onsButton %}
{% from "components/panel/_macro.njk" import onsPanel %}

{% set prevPage = "index" %}
{% set formActionURL = pageInfo.rootPath + "/summary.html" %}
{% block bodyStart %}
    <form method="GET" action="{{formActionURL}}">
{% endblock %}

{% block main %}
    <h1>Submit study</h1>
    {% call onsPanel({
            "type": "warn"
        })
    %}
        You must submit this study to complete it
    {% endcall %}

    <div class="ons-summary">
        <div class="ons-summary__group">
            <h2 class="ons-summary__group-title">Turnover</h2>
            <table id="turnover" class="ons-summary__items js-summary">
            </table>
        </div>
    </div>

    <p class="ons-u-mt-l">By submitting this study you are confirming that, to the best of your knowledge and belief, the details provided are correct.</p>

    {{
        onsButton({
            "text": 'Submit study'
        })
    }}
{% endblock %}

{% block bodyEnd %}
    </form>
    <script src="/prototypes/example/index.js"></script>
{% endblock %}
